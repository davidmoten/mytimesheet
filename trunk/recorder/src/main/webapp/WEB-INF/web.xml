<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Tv Recorder</display-name>

	<listener>
		<listener-class>moten.david.util.tv.servlet.SchedulerServletContextListener</listener-class>
	</listener>

	<servlet>
		<servlet-name>service</servlet-name>
		<servlet-class>moten.david.util.tv.ui.server.ApplicationServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>service</servlet-name>
		<url-pattern>/tv/service</url-pattern>
	</servlet-mapping>


</web-app>
